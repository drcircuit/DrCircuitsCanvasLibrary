{
  "reporter": ["text", "text-summary", "html", "json","lcov"],
  "reports-dir": "./coverage",
  "exclude": [
    "tests/**",
    "examples/**",
    "dist/**",
    "node_modules/**",
    "coverage/**",
    "*.config.js",
    "rollup.config.js"
  ],
  "include": [
    "src/**/*.js"
  ],
  "all": true,
  "check-coverage": true,
  "lines": 70,
  "functions": 35,
  "branches": 65,
  "statements": 70
}